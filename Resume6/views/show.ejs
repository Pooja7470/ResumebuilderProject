<%- include('./partials/header')%>

    <div class="csn ">
        <h3 class="go">All Resumes</h3>

        <div class="resume ">

            <div class="d-left">
                <page size="A4">
                    <div class="div11">
                        <img height="200px" width="200px" class="rr" style="object-fit: cover; border-radius: 50%"
                            src="/images/<%= user.avatar%>" alt="" />
                        <h1 class="htt ">
                            <%= user.username%>
                        </h1>
                        <h5 class="hos ">
                            <%= user.email%>
                        </h5>
                        <h6 class="hos">
                            <%= user.contact%>
                        </h6>
                    </div>
            </div>
            <hr />
            <div class="d-right">
                <h1 class="tin">Education :-</h1>
                <div class=" sn d-flex flex-wrap">
                    <% user.education.forEach((e, i)=> { %>
                        <div class=" card me-3 mb-3" style="width: 18rem">
                            <div class="">
                                <h5 class="card-title">
                                    <%= e.degree%>
                                </h5>
                                <h6 class="card-subtitle mb-2 text-muted">
                                    <%= e.institute%> - <%= e.year%> | <%= e.university%>
                                </h6>
                                <p class="card-text">CGPA/PERCENTAGE<%= e.grade%>
                                </p>
                            </div>
                        </div>
                        <% })%>
                </div>
                <h1 class="tin">Skills :-</h1>
                <div class=" sn d-flex flex-wrap">
                    <% user.skill.forEach((e, i)=> { %>
                        <div class="card me-3 mb-3" style="width: 18rem">
                            <div class="">
                                <h5 class="card-title">
                                    <%= e.skill%>
                                </h5>
                                <p class="card-text">Tecnical skills<br>
                                    <%= e.skill%>
                                </p>

                            </div>
                        </div>
                        <% })%>
                </div>
                <h1 class="tin">Project :-</h1>
                <div class=" sn d-flex flex-wrap">
                    <% user.project.forEach((e, i)=> { %>
                        <div class="  card me-3 mb-3" style="width: 18rem">
                            <div class="">
                                <h5 class="card-title">
                                    <%= e.title%>
                                        <%= e.description%>
                                </h5>
                                <p class="card-text">Tecnical project<br>
                                    <%= e.project%>
                                </p>

                            </div>
                        </div>
                        <% })%>

                </div>

                <h1 class="tin">Experience :-</h1>
                <div class=" sn d-flex flex-wrap">
                    <% user.experience.forEach((e, i)=> { %>
                        <div class="card me-3 mb-3 " style="width: 18rem">
                            <div class="">
                                <h5 class="card-title ">
                                    <%= e.companyname%> , <%= e.designation%> , <%= e.duration%>
                                </h5>
                                <p class="card-text">Tecnical experience<br>
                                    <%= e.experience%>
                                </p>

                            </div>
                        </div>
                        <% })%>
                </div>

                <h1 class="tin">interest :-</h1>
                <div class="sn d-flex flex-wrap">
                    <%user.interest.forEach((e,i)=> { %>
                        <div class=" card me-3 mb-3" style="width: 18rem">
                            <div class="">
                                <h5 class="card-title">
                                    <%= e.interest%>
                                </h5>
                                <p class="card-text">Tecnical interest <%= e.interest%>
                                </p>

                            </div>

                        </div>

                        <% })%>
                </div>



            </div>



            </page>


        </div>
        <button class=" printbtn  btn btn-success">Print</button>
    </div>






    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <link rel="stylesheet" href="https://kendo.cdn.telerik.com/2017.1.223/styles/kendo.common.min.css" />
    <script src="https://kendo.cdn.telerik.com/2017.1.223/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2017.1.223/js/kendo.all.min.js"></script>

    <script src="https://kendo.cdn.telerik.com/2017.1.223/js/jszip.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2017.1.223/js/kendo.all.min.js"></script>

    <script>
        function getPDF(selector) {
            kendo.drawing.drawDOM($(selector)).then(function (group) {
                kendo.drawing.pdf.saveAs(group, "testing.pdf");
            });
        }
    </script>
    <script type="text/javascript">
        $(".printbtn").click(function () {
            getPDF(".resume");
        });
    </script>

    <%- include('./partials/footer')%>

