<%- include('../partials/header')%>
    <div class="cons p-3">
        <ul>
            <li><a href="/education">Eduaction</a></li>
            <li><a href="/skill">Skill</a></li>
            <li><a href="/project">Project</a></li>
            <li><a href="/experience">Experience</a></li>
            <li><a href="/interest">Interest</a></li>
        </ul>
        <hr />
        <div class="drw">

            <form action="/add-project" class="project  mt-5 " method="post">
                <input class="fo " type="text" placeholder="title" name="title">
                <input class="fg" type="text" placeholder="Description" name="Description">
                <input class="fg" type="text" placeholder="technology" name="technology">
                <input class="ft" type="text" placeholder="link" name="link"><br>


                <button class="btns">Add project</button>
            </form>


            <hr />

            <div class=" sn d-flex flex-wrap">
                <% user.project.forEach((e, i)=> { %>
                    <div class=" gn card me-3 mb-3" style="width: 18rem">
                        <div class="card-body">
                            <h5 class="card-title">
                                <%= e.title%>
                                    <%= e.description%>
                            </h5>
                            <p class="card-text">Tecnical project<br>
                                <%= e.project%>
                            </p>
                            <a href="/delete-proj/<%= i%>" class="card-link">Remove</a>

                        </div>
                    </div>
                    <% })%>
            </div>

        </div>

        <%- include('../partials/footer')%>