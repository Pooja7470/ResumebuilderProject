<%- include('../partials/header')%>
    <div class="cons  p-3">
        <ul>
            <li><a href="/education">Eduaction</a></li>
            <li><a href="/skill">Skill</a></li>
            <li><a href="/project">Project</a></li>
            <li><a href="/experience">Experience</a></li>
            <li><a href="/interest">Interest</a></li>
        </ul>
        <hr />
        <div class="drw">
        <form action="/add-edu" class="education mt-5" method="POST">
            <input class="fo" type="text" placeholder="Institute Name" name="institute" />
            <input class="fg" type="text" placeholder="Degree Name" name="degree" />
            <input  class="fg" type="text" placeholder="University Name" name="university" />
            <input  class="ft" type="text" placeholder="Year" name="year" />
            <input  class="fn" type="text" placeholder="Grade/Percentage" name="grade" />

            <button class="btns">Add Education</button>
        </form>
        <hr />
        <div class=" sn d-flex flex-wrap">
            <% user.education.forEach((e, i)=> { %>
                <div class="gn card me-3 mb-3" style="width: 18rem">
                    <div class="card-body">
                        <h5 class="card-title">
                            <%= e.degree%>
                        </h5>
                        <h6 class="card-subtitle mb-2 text-muted">
                            <%= e.institute%> - <%= e.year%> | <%= e.university%>
                        </h6>
                        <p class="card-text">CGPA/PERCENTAGE <%= e.grade%>
                        </p>
                        <a href="/delete-edu/<%= i%>" class="card-link">Remove</a>
                    </div>
                </div>
                <% })%>
        </div>
    </div>
    </div>

    <%- include('../partials/footer')%>
       

















